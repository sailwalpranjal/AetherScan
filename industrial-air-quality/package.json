{
  "name": "industrial-air-quality",
  "version": "1.0.0",
  "description": "Advanced industrial air quality monitoring system with quantum-enhanced data processing",
  "private": true,
  "author": {
    "name": "Air Quality Monitoring Team",
    "email": "team@airquality.monitor"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/industrial-air-quality.git"
  },
  "keywords": [
    "air-quality",
    "environmental-monitoring",
    "industrial-emissions",
    "satellite-data",
    "quantum-computing",
    "real-time-monitoring"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev --turbo",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:strict": "eslint --max-warnings 0 .",
    "type-check": "tsc --noEmit --incremental",
    "format": "prettier --write . --ignore-path .gitignore",
    "format:check": "prettier --check . --ignore-path .gitignore",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next out dist build coverage",
    "clean:deps": "rimraf node_modules package-lock.json && npm install",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "build:docker": "docker build -t industrial-air-quality .",
    "docker:run": "docker run -p 3000:3000 industrial-air-quality",
    "healthcheck": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.3",
    "@react-three/drei": "^9.96.1",
    "@react-three/fiber": "^8.15.19",
    "@vercel/analytics": "^1.1.2",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.28",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.542.0",
    "next": "^14.2.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.8.1",
    "sharp": "^0.33.2",
    "three": "^0.161.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.32",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/three": "^0.161.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "compression-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.32",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type-check",
      "pre-push": "npm run lint:strict && npm run build"
    }
  }
}
